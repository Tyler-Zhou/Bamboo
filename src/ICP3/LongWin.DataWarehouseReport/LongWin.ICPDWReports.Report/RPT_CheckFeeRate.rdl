    <DataSet Name="DataSourceICP2005">
      <Fields>
        <Field Name="rate">
          <DataField>rate</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="inrate">
          <DataField>inrate</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Currency">
          <DataField>Currency</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MainCurrency">
          <DataField>MainCurrency</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="billNo">
          <DataField>billNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Email">
          <DataField>Email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSourceICP2005</DataSourceName>
        <CommandText>SELECT DISTINCT b.rate,d.inrate,b.Currency,C.MainCurrency,A.[No] as billNo
 ,CASE WHEN RTRIM(LTRIM(ISNULL(F.Email,'')))='' THEN 'think@longwin.net' ELSE f.Email END AS Email
FROM dbo.T_FIN_CustomerBill A
INNER JOIN dbo.T_FIN_OperationCharge B ON A.Id = B.CustomerBillId
INNER JOIN dbo.T_PUB_Configure C ON C.CompanyId =A.CompanyId
INNER JOIN dbo.T_FWD_BillInfo E ON E.ChargeBillId = A.ID
LEFT JOIN dbo.T_SYS_Users F ON F.ID = E.CreateBy
INNER JOIN dbo.T_PUB_ExchangeRate  D ON D.CompanyId = A.CompanyId 
AND D.TargetCurrency=C.MainCurrency AND D.SourceCurrency = B.Currency
AND d.BeginTime &lt;= CAST(A.CreateDate AS DATETIME) 
AND d.EndTime &gt; CAST(A.CreateDate AS DATETIME) 
WHERE a.createdate&gt;'2009-01-01'
AND  CAST(b.rate AS DECIMAL(19,2)) &lt;&gt; CAST(d.inrate AS DECIMAL(19,2))
and CASE WHEN RTRIM(LTRIM(ISNULL(F.Email,'')))='' THEN 'think@longwin.net' ELSE f.Email END =@Email</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Email">
            <Value>=Parameters!Email.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
