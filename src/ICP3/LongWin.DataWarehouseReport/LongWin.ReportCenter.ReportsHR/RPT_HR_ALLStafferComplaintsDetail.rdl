      <Query>
        <DataSourceName>HRReportSource</DataSourceName>
        <CommandText>SELECT [Complainant]
      ,[ReceiverDate]
      ,[Reason]
      ,[GeneralDeptViews]
      ,[ComplaineesStatement]
      ,[ComplaineesDeptManagerViews]
      ,[ComplaineesCompanyManagerViews]
      ,[Solutions]
      ,B.StafferId
      ,C.[Name] AS StafferName
  FROM [ICP2005].[dbo].[T_HR_StafferComplaints] A
       INNER JOIN T_HR_StafferComplaintsRelations B ON A.ID = B.ComplaintsID
       INNER JOIN dbo.T_HR_StafferBaseInfo C ON C.ID = StafferId
  WHERE StafferId=@StafferId</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StafferId">
            <Value>=Parameters!StafferId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
