          <knownType type="ICP.FAM.ServiceInterface.DataObjects.CurrencyBillList,ICP.FAM.ServiceInterface" />
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.InvoiceList,ICP.FAM.ServiceInterface" />
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.ReleaseRCList,ICP.FAM.ServiceInterface" />
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.WriteOffItemList,ICP.FAM.ServiceInterface" />
          <knownType type="ICP.FRM.ServiceInterface.DataObjects.SearchOceanRateList,ICP.FRM.ServiceInterface" />
          <knownType type="ICP.FRM.ServiceInterface.DataObjects.FrmUnitRateInfo,ICP.FRM.ServiceInterface" />
          <knownType type="ICP.FRM.ServiceInterface.DataObjects.FrmUnitRateList,ICP.FRM.ServiceInterface" />
          <knownType type="ICP.FRM.ServiceInterface.DataObjects.FrmUnitRateInfo,ICP.FRM.ServiceInterface" />
          <knownType type="ICP.WF.ServiceInterface.WFBusinessList,ICP.WF.ServiceInterface" />
          <knownType type="ICP.FRM.ServiceInterface.DataObjects.SearchTruckRateList,ICP.FRM.ServiceInterface" />
          <knownType type="ICP.Framework.CommonLibrary.Common.DataPageInfo,ICP.Framework.CommonLibrary" />
        </add>
        <add type="ICP.FAM.ServiceInterface.DataObjects.OperationParameter,ICP.FAM.ServiceInterface">
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.AirParameter,ICP.FAM.ServiceInterface" />
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.CustomsParameter,ICP.FAM.ServiceInterface" />
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.OtherParameter,ICP.FAM.ServiceInterface" />
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.OceanParameter,ICP.FAM.ServiceInterface" />
        </add>
      </declaredTypes>
    </dataContractSerializer>
  </system.runtime.serialization>
  <CallbackServiceSection>
    <Services>
      <add Id="ISubscriptionPublishService" ServiceType="ICP.SubscriptionPublish.ServiceInterface.ISubscriptionPublishService,ICP.SubscriptionPublish.ServiceInterface" CallbackType="ICP.SubscriptionPublish.ServiceInterface.ISubscriptionPublishNotifyService,ICP.SubscriptionPublish.ServiceInterface" CallbackInstanceType="ICP.SubscriptionPublish.Client.SubscriptionPublishNotifyClientService,ICP.SubscriptionPublish.Client" IsCallbackInstanceExists="true" EventName="" />
    </Services>
  </CallbackServiceSection>
  <system.serviceModel>
    <bindings />
    <client />
  </system.serviceModel>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>