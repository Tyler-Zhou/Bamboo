<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="addins" type="ICP.Controls.AddInHandler,ICP.Controls" />
    <section name="CallbackServiceSection" type="ICP.Framework.CommonLibrary.Client.CallbackServiceConfigurationSection,ICP.Framework.CommonLibrary" />
  </configSections>
  <appSettings>
    <add key="FileServerUrl" value="http://127.0.0.1:11/ICPFiles.zip" />
    <add key="WebSiteUrl" value="http://culture.cityocean.com:6688/index.aspx" />
    <add key="EnableNls" value="true" />
    <add key="serviceBaseAddress" value="http://localhost:80/cityOceanService" />
    <add key="tcpServiceBaseAddress" value="net.tcp://localhost:8087/cityOceanService" />
    <add key="ApplicationName" value="ICPMain.exe" />
    <add key="UpdateApplicationName" value="ICPUpdate.exe" />
    <add key="serviceNameSpace" value="www.cityocean.com" />
    <add key="TcpServiceNameSpace" value="www.cityocean.com" />
    <!-- 需要流式传输的服务类型名称，服务名以逗号隔开，区分大小写-->
    <add key="streamedRequestServices" value="" />
    <add key="sessionRequestServices" value="ISqlDataSynchronizationService" />
    <!-- 压缩阈值，以KB为单位 为方便测试，测试时以B为单位-->
    <add key="CompressionSize" value="500" />
    <add key="InternalDomain" value="cityocean.com,topshipping.com,harvest.com" />
    <add key="ICPHostServiceBaseAddress" value="net.pipe://127.0.0.1/LocalService" />
    <add key="EmailCenterHostServiceBaseAddress" value="net.pipe://127.0.0.1/EmailCenterService" />
    <add key="EmailCenterName" value="ICPMailCenter.exe" />
    <add key="PropagateContext" value="true" />
    <add key="FilterModules" value="ICP.MailCenter.UI.dll,ICP.MailCenter.Business.UI" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <system.runtime.serialization>
    <dataContractSerializer>
      <declaredTypes>
        <add type="ICP.Framework.CommonLibrary.Common.PageList,ICP.Framework.CommonLibrary">
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.BusinessList,ICP.FAM.ServiceInterface" />
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.JournalList,ICP.FAM.ServiceInterface" />
          <knownType type="ICP.FAM.ServiceInterface.DataObjects.AgnetBillCheckList,ICP.FAM.ServiceInterface" />
