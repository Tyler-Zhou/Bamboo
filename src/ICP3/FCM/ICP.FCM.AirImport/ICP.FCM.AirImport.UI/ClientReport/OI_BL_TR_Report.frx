  {
    private void Data1_AfterData(object sender, EventArgs e)
    {
      if(String.IsNullOrEmpty(((String)Report.GetColumnValue(&quot;ReportSource.Header&quot;)))==false)
      {
            txtHand.Text =   ((String)Report.GetColumnValue(&quot;ReportSource.Header&quot;));
      }
      if(String.IsNullOrEmpty(((String)Report.GetColumnValue(&quot;ReportSource.CompanyFax&quot;)))==false)
      {
        txtFax.Text =&quot;Fax:&quot;+((String)Report.GetColumnValue(&quot;ReportSource.CompanyFax&quot;));
      }
      if(String.IsNullOrEmpty(((String)Report.GetColumnValue(&quot;ReportSource.CompanyTel&quot;)))==false)
      {
        txtTel.Text = &quot;Tel:&quot;+((String)Report.GetColumnValue(&quot;ReportSource.CompanyTel&quot;));
      }
      
      if((String)Report.GetColumnValue(&quot;ReportSource.PaymentTerm&quot;)==&quot;CC&quot;)
        txtPP.Text = String.Empty;
      else
        txtCC.Text = String.Empty;
        
    }
  }
}
