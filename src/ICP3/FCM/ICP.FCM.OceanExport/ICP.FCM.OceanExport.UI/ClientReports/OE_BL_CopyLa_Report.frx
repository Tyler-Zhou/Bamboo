    private void Page1_StartPage(object sender, EventArgs e)
    {
      if((String)Report.GetColumnValue(&quot;ReportSource.PaymentTerm&quot;)==&quot;CC&quot;)
        Text43.Text  = &quot;Freight  collect&quot;;
      else
        Text43.Text  = &quot;Freight  prepaid&quot;;
      
      if((String)Report.GetColumnValue(&quot;ReportSource.FreightAndCharges&quot;).ToString().ToUpper()==&quot;TRUE&quot;)
       Text11.Text  = &quot;&quot;;
      else
        Text11.Text  = &quot;JDY INTERNATIONAL INC.\r\n2/F, 1350 VALLEY VISTA DR,DIAMOND BAR, CA 91765\r\nFMC#  025893NF\r\nTEL:909-569-6966&quot;;
    }
  }
}
